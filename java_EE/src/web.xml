<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>TestApp</display-name>
  <description>Mob Filter Test</description>	
	
<!-- filters -->
 
 <filter>
  <filter-name>mobFilter</filter-name>
  <filter-class>com.zuznow.mobFilter</filter-class>
  <init-param> 
    <param-name>domainID</param-name> 
    <param-value></param-value> 
  </init-param> 
  <init-param> 
    <param-name>APIKey</param-name> 
    <param-value></param-value> 
  </init-param>
   <init-param> 
    <param-name>originalDomain</param-name> 
    <param-value></param-value> 
  </init-param>
   <init-param> 
    <param-name>apiServers</param-name> 
    <param-value></param-value> 
  </init-param> 
  <init-param>
	<param-name>exclude</param-name>
	<param-value></param-value>
  </init-param> 
</filter>

<filter-mapping>
  <filter-name>mobFilter</filter-name>
  <url-pattern>/*</url-pattern>
</filter-mapping>   

</web-app>
